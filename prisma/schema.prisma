generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model User {
  userId                String @id @default(uuid())
  nickname              String
  email                 String @unique
  favoriteTheme         String
  favoriteSecondTheme   String
  password              String
}

model Workspace {
  workspaceId String @id @default(uuid())
  name        String
  icon        String
  link        String
  blocks      Json
}